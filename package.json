{
  "name": "tinify-client",
  "version": "2.0.2",
  "description": "Compress your images not only intelligently but also to the EXTREME using tinify!",
  "files": [
    "src/*"
  ],
  "bin": {
    "tinify-client": "src/index.js",
    "tinify": "src/index.js"
  },
  "scripts": {
    "test": "LANG=zh-CN node src https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ --max-count=1",
    "test:e2e": "LANG=zh-CN node src https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ --output=./alipay-logo.png -verbose -no-base64 --max-count=1"
  },
  "keywords": [
    "tinify",
    "compress",
    "optimize",
    "tinypng",
    "tinyjpg",
    "picture",
    "image"
  ],
  "author": "",
  "homepage": "https://github.com/legend80s/tinify-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legend80s/tinify-client.git"
  },
  "license": "MIT",
  "dependencies": {
    "cli-aid": "^3.0.1",
    "clipboardy": "^2.3.0",
    "image-to-base64": "^2.1.0",
    "ora": "^5.0.0",
    "tinify": "^1.6.0-beta.2"
  },
  "devDependencies": {
    "git-commit-msg-linter": "^2.4.4"
  }
}
