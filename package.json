{
  "name": "tinify-client",
  "version": "3.0.0",
  "description": "Compress your images not only intelligently but also to the EXTREME using tinify!",
  "files": [
    "src/*"
  ],
  "bin": {
    "tinify-client": "src/index.js",
    "tinify": "src/index.js"
  },
  "scripts": {
    "preversion": "npm run test",
    "test": "jest && npm run test:compress-and-base64 && npm run test:compress-no-base64 && npm run test:compress-and-base64",
    "test:concurrently": "concurrently -n xiena,alipay 'xtinify-client https://4img.hitv.com/preview/star_images/2020/xiena/xiena20200401155541958.jpg -m=1' 'node src https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ -m=1'",
    "test:compress-and-base64": "LANG=zh-CN node src https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ --max-count=1 -o=./",
    "test:base64": "LANG=zh-CN node src base64 https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ",
    "test:compress-no-base64": "LANG=zh-CN node src https://gw.alipayobjects.com/mdn/member_frontWeb/afts/img/A*h7o9Q4g2KiUAAAAAAAAAAABkARQnAQ --output=./alipay-logo.png -verbose -no-base64 --max-count=1"
  },
  "keywords": [
    "tinify",
    "compress",
    "optimize",
    "tinypng",
    "tinyjpg",
    "picture",
    "image"
  ],
  "author": "",
  "homepage": "https://github.com/legend80s/tinify-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legend80s/tinify-client.git"
  },
  "license": "MIT",
  "tnpm": {
    "mode": "yarn"
  },
  "dependencies": {
    "@legend80s/image-to-base64": "^1.0.0",
    "cli-aid": "^3.1.0",
    "clipboardy": "^2.3.0",
    "concurrently": "^5.3.0",
    "glob": "^7.1.6",
    "ora": "^5.0.0",
    "tinify": "^1.6.0-beta.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "git-commit-msg-linter": "^2.4.4",
    "jest": "^26.6.1"
  }
}
